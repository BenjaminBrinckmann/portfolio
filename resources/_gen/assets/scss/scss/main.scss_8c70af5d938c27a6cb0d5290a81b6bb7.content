body{font-family:'IBM Plex Sans', sans-serif}@media screen and (min-width: 48em){body{font-size:1.25em}}@media screen and (min-width: 30em){.u-grid{display:grid;min-height:100%;grid-template:"header" fit-content(100%) "main" 1fr "footer" fit-content(100%)/1fr}.u-grid__header{display:grid;grid-template:"logo" 1fr "nav" 1fr / 1fr}}@media screen and (min-width: 48em){.u-grid__header{grid-template:"logo nav" /  580px 1fr}}.u-contain{max-width:80em;width:100%;margin-left:auto;margin-right:auto}.logo__main{width:12em}@media screen and (min-width: 30em){.logo{grid-area:logo}}@media screen and (min-width: 48em){.m-nav{grid-area:nav;justify-self:right;align-self:center}}.o-header{text-align:center}@media screen and (min-width: 48em){.o-header{text-align:left;grid-area:header}}@media screen and (min-width: 30em){.o-gallery{column-count:2}}@media screen and (min-width: 48em){.o-gallery{column-count:3}}.o-gallery__item{display:inline-block;width:100%}.o-gallery__item img{width:100%;box-shadow:#dddddd 2px 2px 6px}@media screen and (min-width: 30em){.o-gallery__item{padding-bottom:1em}}.o-gallery__span-2{background-color:red;border:green 5px}
